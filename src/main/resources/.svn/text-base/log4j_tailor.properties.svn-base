log4j.appender.R=org.apache.log4j.RollingFileAppender 
log4j.appender.R.File=logs/tailor.log
log4j.appender.R.MaxBackupIndex=1 
log4j.appender.R.MaxFileSize=20000KB 
log4j.appender.R.encoding=UTF-8
log4j.appender.R.layout=org.apache.log4j.PatternLayout 
log4j.appender.R.layout.ConversionPattern=[tailor] %d{yy-MM-dd HH\:mm\:ss} %p %c{2}\: %m%n

log4j.appender.job=org.apache.log4j.FileAppender
log4j.appender.job.File=logs/tailor_job.log
log4j.appender.job.MaxFileSize=20000KB 
log4j.appender.job.encoding=UTF-8
log4j.appender.job.layout=org.apache.log4j.PatternLayout 
log4j.appender.job.layout.ConversionPattern=[tailor_job] %d{yy-MM-dd HH\:mm\:ss} %p %c{2}\: %m%n


log4j.appender.stdout=org.apache.log4j.ConsoleAppender 
log4j.appender.stdout.layout=org.apache.log4j.PatternLayout 
log4j.appender.stdout.layout.ConversionPattern=[tailor] %d{yy-MM-dd HH\:mm\:ss} %p %c{2}\: %m%n

log4j.logger.com.renren.tailor.schedule=INFO,job

log4j.logger.com.renren.tailor=DEBUG

#log4j.category.com.renren.sched.client=FATAL

log4j.rootLogger=INFO,stdout, R
log4j.threshhold=ALL
